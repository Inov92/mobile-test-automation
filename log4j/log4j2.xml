<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Appenders>
		<Console name="STDOUT" target="SYSTEM_OUT">
			<PatternLayout pattern="%d %-5p [%t] %M (%F:%L) - %m%n"/>
		</Console>
	</Appenders>
	<Loggers>
		<Logger name="test.HelloAppium" level="info"/>
		<Root level="info">
			<AppenderRef ref="STDOUT"/>
		</Root>
	</Loggers>
</Configuration>